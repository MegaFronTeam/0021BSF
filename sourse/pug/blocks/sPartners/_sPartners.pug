mixin sPartners()
	- let data = baseLang.sPartners
	if data
		// start sPartners
		+b.SECTION.sPartners.section#sPartners&attributes(attributes)
			.container
				+b.section-title
					h2= data.h2
				+e.row.row
					each item in data.items
						+e.col.col-6.col-lg-3
							+e.item
								+e.wrap
									+e.logo-wrap
										.img-wrap-center
											+img(`img/sPartners-img-${item[0]}.png`)
									+e.text!= item[1]
								+e.content
									p!= item[2]
		// end sPartners